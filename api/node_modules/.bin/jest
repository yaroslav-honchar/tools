#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules/jest/bin/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules/jest/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules/jest/bin/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules/jest/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/jest@30.0.5_@types+node@22.17.0_ts-node@10.9.2_@types+node@22.17.0_typescript@5.9.2_/node_modules:/Users/yaroslav_h/Projects/Tool/dev-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
